{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },

  "rules": {
      "singleQuote": true,
      "semi": false,
      "trailingComma": "es5",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "_"
      }
    ],
    "sort-vars": "error"
  }
}
